                    <ul>

                        <li style="padding-top:45px">
                            <a asp-action="Index" asp-controller="Home" class="active"><i class="fa fa-home nav_icon"></i>Trang Quản Lý</a>

                        </li>


                        <li style="padding-top:45px">
                            <a asp-controller="Categories" asp-action="Index"><i class="fa fa-book nav_icon"></i>Categories </a>

                        </li>



                        <li style="padding-top:45px">
                            <a asp-controller="suppliers" asp-action="Index"><i class="fa fa-th-large nav_icon"></i>Supplier </a>
                        </li>


                        <li style="padding-top:45px">
                            <a asp-controller="Customers" asp-action="Index"><i class="fa fa-table nav_icon"></i>Custommers </a>
                        </li>


                        <li style="padding-top:45px">
